<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="543.0" prefWidth="807.0" stylesheets="@../styles/main.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PrintController">
   <children>
      <ImageView fx:id="image" fitHeight="430.0" fitWidth="806.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS" />
      <Region prefHeight="200.0" prefWidth="200.0" />
      <HBox alignment="CENTER" prefWidth="807.0" spacing="40.0">
         <children>
            <Button mnemonicParsing="false" onAction="#cancel" prefWidth="100.0" styleClass="button-secondary" text="Cancel" />
            <Button fx:id="printButton" minHeight="-Infinity" mnemonicParsing="false" onAction="#print" prefWidth="173.0" styleClass="button-accent" text="Print" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
